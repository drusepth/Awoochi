<h1>Editing unlock</h1>

<% form_for(@unlock) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p>
    <%= f.label :achievement_id %><br />
    <%= f.text_field :achievement_id %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @unlock %> |
<%= link_to 'Back', unlocks_path %>